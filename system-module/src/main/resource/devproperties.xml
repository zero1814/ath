<?xml version="1.0" encoding="UTF-8"?>
<properties>
	<property name="image">
		<attributes name="upload">
			<!-- 访问地址 -->
			<attribute name="url"></attribute>
			<!-- 相对路径 -->
			<attribute name="relative_path"></attribute>
			<!-- 存储路径 -->
			<attribute name="real_path"></attribute>
			<!-- 图片类型限制,用','隔开 -->
			<attribute name="type"></attribute>
		</attributes>
	</property>
	<property name="file">
		<attributes name="upload">
			<!-- 访问地址 -->
			<attribute name="url"></attribute>
			<!-- 相对路径 -->
			<attribute name="relative_path"></attribute>
			<!-- 存储路径 -->
			<attribute name="real_path"></attribute>
			<!-- 文件类型限制,用','隔开 -->
			<attribute name="type"></attribute>
		</attributes>
	</property>
</properties>
