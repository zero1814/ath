<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.module.mapper.logger.user.LmOperateMapper">
	<resultMap id="BaseResultMap" type="org.module.model.logger.user.LmOperate">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="uid" property="uid" jdbcType="VARCHAR" />
		<result column="operate_type" property="operateType" jdbcType="VARCHAR" />
		<result column="class_path" property="classPath" jdbcType="VARCHAR" />
		<result column="request_data" property="requestData" jdbcType="VARCHAR" />
		<result column="response_data" property="responseData"
			jdbcType="VARCHAR" />
		<result column="create_user" property="createUser" jdbcType="VARCHAR" />
		<result column="create_time" property="createTime" jdbcType="VARCHAR" />
	</resultMap>
	<sql id="Base_Column_List">
		id, uid, operate_type, class_path, request_data,
		response_data,
		create_user, DATE_FORMAT(create_time,'%Y-%m-%d
		%H:%i:%s') as
		create_time
	</sql>
	<select id="selectByUserCode" resultMap="BaseResultMap"
		parameterType="java.lang.String">
		select
		<include refid="Base_Column_List" />
		from loggermodule.lm_operate
		where create_user = #{createUser,jdbcType=VARCHAR}
	</select>
	<insert id="insertSelective" parameterType="org.module.model.logger.user.LmOperate">
		insert into loggermodule.lm_operate
		<trim prefix="(" suffix=")" suffixOverrides=",">
			uid,
			operate_type,
			class_path,
			request_data,
			response_data,
			create_user,
			create_time,
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{uid,jdbcType=VARCHAR},
			#{operateType,jdbcType=VARCHAR},
			#{classPath,jdbcType=VARCHAR},
			#{requestData,jdbcType=VARCHAR},
			#{responseData,jdbcType=VARCHAR},
			#{createUser,jdbcType=VARCHAR},
			STR_TO_DATE(#{createTime,jdbcType=VARCHAR},'%Y-%m-%d
			%H:%i:%s'),
		</trim>
	</insert>
</mapper>