<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.module.mapper.logger.store.LmStoreFlowMapper">
	<resultMap id="BaseResultMap" type="org.module.model.logger.store.LmStoreFlow">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="uid" property="uid" jdbcType="VARCHAR" />
		<result column="store_code" property="storeCode" jdbcType="VARCHAR" />
		<result column="flow_status" property="flowStatus" jdbcType="VARCHAR" />
		<result column="create_user" property="createUser" jdbcType="VARCHAR" />
		<result column="create_time" property="createTime" jdbcType="VARCHAR" />
		<result column="store_json" property="storeJson" jdbcType="LONGVARCHAR" />
	</resultMap>
	<sql id="Base_Column_List">
		id, uid, store_code, flow_status,store_json, create_user,
		DATE_FORMAT(create_time,'%Y-%m-%d
		%H:%i:%s') as
		create_time
	</sql>
	<select id="selectByCode" resultMap="ResultMapWithBLOBs"
		parameterType="java.lang.String">
		select
		<include refid="Base_Column_List" />
		from loggermodule.lm_store_flow
		where store_code = #{storeCode,jdbcType=VARCHAR}
	</select>
	<insert id="insertSelective" parameterType="org.module.model.logger.store.LmStoreFlow">
		insert into loggermodule.lm_store_flow
		<trim prefix="(" suffix=")" suffixOverrides=",">
			uid,
			store_code,
			flow_status,
			create_user,
			create_time,
			store_json,
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{uid,jdbcType=VARCHAR},
			#{storeCode,jdbcType=VARCHAR},
			#{flowStatus,jdbcType=VARCHAR},
			#{createUser,jdbcType=VARCHAR},
			STR_TO_DATE(#{createTime,jdbcType=VARCHAR},'%Y-%m-%d
			%H:%i:%s'),
			#{storeJson,jdbcType=LONGVARCHAR},
		</trim>
	</insert>
</mapper>